./scramble.py -d -64    --salt Steven+Donald+Parker Sample_file.txt         Sample_scrambled_64.txt   >   Scrambling_64_out.txt
./scramble.py -d -64 -u --salt Bmg0QgB0caSAUAw1aEYo Sample_scrambled_64.txt Sample_unscrambled_64.txt > Unscrambling_64_out.txt
diff Sample_file.txt Sample_unscrambled_64.txt
